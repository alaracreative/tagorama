{
    "name": "alaracreative/tagorama",
    "description": "An Eloquent tagging package for Laravel. Just a demo, don't expect much.",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Zirkel",
            "email": "scott@alaracreative.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "Alaracreative\\Tagorama\\": "src/"
      }
    },
    "require": {
      "php": ">=7.1.0",
      "illuminate/database": ">=5.6",
      "illuminate/support": ">5.6"
    },
    "autoload-dev": {
      "psr-4": {
        "Alaracreative\\Tagorama\\Tests\\": "tests/"
      },
      "classmap": [
        "tests/TestCase.php",
        "tests/stubs/LessonStub.php",
        "tests/stubs/TagStub.php"
      ]
    },
    "require-dev": {
      "orchestra/testbench": "~3.6",
      "phpunit/phpunit": "~7.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Alaracreative\\Tagorama\\ServiceProvider"
        ]
      }
    }
}
